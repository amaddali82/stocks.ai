{
  "dashboard": {
    "title": "Trading System - Real-time Performance",
    "tags": ["trading", "performance"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Total P&L",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(trading_pnl_total)",
            "legendFormat": "Total P&L"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 2,
        "title": "Prediction Accuracy",
        "type": "gauge",
        "targets": [
          {
            "expr": "avg(prediction_accuracy)",
            "legendFormat": "Accuracy"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 3,
        "title": "Active Positions",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(active_positions_count)",
            "legendFormat": "Positions"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 6,
          "h": 4
        }
      },
      {
        "id": 4,
        "title": "Order Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(orders_successful_total[5m]) / rate(orders_total[5m])",
            "legendFormat": "Success Rate"
          }
        ],
        "gridPos": {
          "x": 6,
          "y": 8,
          "w": 6,
          "h": 4
        }
      },
      {
        "id": 5,
        "title": "Market Data Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(data_ingestion_message_latency_seconds_bucket[5m]))",
            "legendFormat": "P95 Latency"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 6,
        "title": "Sharpe Ratio",
        "type": "gauge",
        "targets": [
          {
            "expr": "trading_sharpe_ratio",
            "legendFormat": "Sharpe Ratio"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 8,
          "h": 6
        }
      },
      {
        "id": 7,
        "title": "Max Drawdown",
        "type": "gauge",
        "targets": [
          {
            "expr": "trading_max_drawdown",
            "legendFormat": "Max Drawdown"
          }
        ],
        "gridPos": {
          "x": 8,
          "y": 16,
          "w": 8,
          "h": 6
        }
      },
      {
        "id": 8,
        "title": "Win Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "sum(winning_trades_total) / sum(total_trades)",
            "legendFormat": "Win Rate"
          }
        ],
        "gridPos": {
          "x": 16,
          "y": 16,
          "w": 8,
          "h": 6
        }
      }
    ]
  }
}
